<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="83dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp"
        app:srcCompat="@drawable/macromap_testlogo" />

    <TextView
        android:id="@+id/textViewDate"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:gravity="center"
        android:text="September 10, 2023"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:layout_editor_absoluteX="16dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:padding="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewDate"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="16dp">


        <TextView
            android:id="@+id/textView_calories"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Calories"
            android:textSize="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView_calories"
            app:layout_constraintTop_toTopOf="@+id/imageView_calories" />

        <TextView
            android:id="@+id/textView_calroieCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="current#"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="@+id/progressBar_calories"
            app:layout_constraintTop_toTopOf="@+id/imageView_calories" />

        <ProgressBar
            android:id="@+id/progressBar_calories"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="200dp"
            android:layout_height="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView_calories"
            app:layout_constraintStart_toStartOf="@+id/textView_calories" />

        <EditText
            android:id="@+id/editText_goal_calories"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginStart="4dp"
            android:inputType="number"
            android:text="goal#"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="@+id/progressBar_calories"
            app:layout_constraintStart_toEndOf="@+id/progressBar_calories" />

        <ImageView
            android:id="@+id/imageView_calories"
            android:layout_width="62dp"
            android:layout_height="54dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_dialog_info" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:padding="16dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="16dp">

        <!-- Add your UI elements here -->

        <TextView
            android:id="@+id/textView_fat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Fat"
            android:textSize="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView_fat"
            app:layout_constraintTop_toTopOf="@+id/imageView_fat" />

        <TextView
            android:id="@+id/textView_fatCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="current#"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="@+id/progressBar_fat"
            app:layout_constraintTop_toTopOf="@+id/imageView_fat" />

        <ProgressBar
            android:id="@+id/progressBar_fat"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="200dp"
            android:layout_height="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView_fat"
            app:layout_constraintStart_toStartOf="@+id/textView_fat" />

        <EditText
            android:id="@+id/editText_goal_fat"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginStart="4dp"
            android:inputType="number"
            android:text="goal#"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="@+id/progressBar_fat"
            app:layout_constraintStart_toEndOf="@+id/progressBar_fat" />

        <ImageView
            android:id="@+id/imageView_fat"
            android:layout_width="62dp"
            android:layout_height="54dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_dialog_info" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:padding="16dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="16dp">


        <TextView
            android:id="@+id/textView_carbs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Carbs"
            android:textSize="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView_carbs"
            app:layout_constraintTop_toTopOf="@+id/imageView_carbs" />

        <TextView
            android:id="@+id/textView_carbCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="current#"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="@+id/progressBar_carbs"
            app:layout_constraintTop_toTopOf="@+id/imageView_carbs" />

        <ProgressBar
            android:id="@+id/progressBar_carbs"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="200dp"
            android:layout_height="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView_carbs"
            app:layout_constraintStart_toStartOf="@+id/textView_carbs" />

        <EditText
            android:id="@+id/editText_goal_carbs"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginStart="4dp"
            android:inputType="number"
            android:text="goal#"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="@+id/progressBar_carbs"
            app:layout_constraintStart_toEndOf="@+id/progressBar_carbs" />

        <ImageView
            android:id="@+id/imageView_carbs"
            android:layout_width="62dp"
            android:layout_height="54dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_dialog_info" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:padding="16dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="16dp">

        <!-- Add your UI elements here -->

        <TextView
            android:id="@+id/textView_protein"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Protein"
            android:textSize="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView_protein"
            app:layout_constraintTop_toTopOf="@+id/imageView_protein" />

        <TextView
            android:id="@+id/textView_proteinCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="current#"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="@+id/progressBar_protein"
            app:layout_constraintTop_toTopOf="@+id/imageView_protein" />

        <ProgressBar
            android:id="@+id/progressBar_protein"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="200dp"
            android:layout_height="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView_protein"
            app:layout_constraintStart_toStartOf="@+id/textView_protein" />

        <EditText
            android:id="@+id/editText_goal_protein"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginStart="4dp"
            android:inputType="number"
            android:text="goal#"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="@+id/progressBar_protein"
            app:layout_constraintStart_toEndOf="@+id/progressBar_protein" />

        <ImageView
            android:id="@+id/imageView_protein"
            android:layout_width="62dp"
            android:layout_height="54dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_dialog_info" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="16dp">

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:text="Add Food"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button_foodList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:text="Food List"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

